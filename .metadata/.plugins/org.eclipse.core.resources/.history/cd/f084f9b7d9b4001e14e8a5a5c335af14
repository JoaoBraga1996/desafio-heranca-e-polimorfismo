package educ360;

import java.util.Random;
import java.util.Scanner;

public class ProgramaTeste {

	public static void main(String[] args) {

		int[] n = new int[3];
		n[0] = 2;
		n[1] = 4;
		n[2] = 6;

		double maior = 0;

		for (int i = 0; i < n.length; i++) {
			if (n[i] > maior) {
				maior = n[i];

			}

		}

		System.out.println(maior);

	}
}
